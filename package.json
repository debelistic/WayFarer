{
  "name": "wayfarer-master",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking server",
  "main": "server/app.js",
  "type": "module",
  "scripts": {
    "test": "set NODE_ENV=test && npm run seeder && mocha -r @babel/register -r @babel/polyfill server/tests/*.js --exit && npm run rollback",
    "devstart": "set NODE_ENV=development && npm run seeder && babel-watch server/app.js",
    "start": "set NODE_ENV=production && npm run migrate && node server/bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "production": "npm run migrate && node server/bin/production",
    "migrate": "npm run create-tables",
    "create-tables": "babel-node server/models/migrations/createTables",
    "insert": "npm run insertUsers && npm run insertBuses && npm run insertBookings && npm run insertTrips",
    "insertUsers": "babel-node server/models/seeders/user_seeder",
    "insertBuses": "babel-node server/models/seeders/bus_seeder",
    "insertBookings": "babel-node server/models/seeders/book_seeder",
    "insertTrips": "babel-node server/models/seeders/trip_seeder",
    "rollback": "babel-node server/models/migrations/dropTables",
    "seeder": "npm run build && npm run rollback && npm run create-tables && npm run insert",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "generate-report": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debelistic/WayFarer.git"
  },
  "keywords": [
    "wayfarer",
    "bus",
    "transportation",
    "andela",
    "blp"
  ],
  "author": "Victor Awotidebe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/debelistic/WayFarer/issues"
  },
  "homepage": "https://github.com/debelistic/WayFarer#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.1",
    "codecov": "^3.5.0",
    "coveralls": "^3.0.5",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.4.4",
    "babel-watch": "^7.0.0",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "compression": "^1.7.4",
    "core-js": "^3.1.4",
    "datauri": "^2.0.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-formidable": "^1.2.0",
    "helmet": "^3.16.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.0",
    "make-runnable": "^1.3.6",
    "mocha": "^6.1.3",
    "morgan": "^1.9.1",
    "nodemailer": "^6.1.0",
    "pg": "^7.9.0",
    "regenerator-runtime": "^0.13.2"
  },
  "engines": {
    "node": "12.5.0",
    "npm": "6.9.0"
  }
}
