{
  "name": "wayfarer-master",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking server",
  "main": "server/app.js",
  "type": "module",
  "scripts": {
    "test": "set NODE_ENV=test && npm run mockdata && mocha -r @babel/register -r @babel/polyfill test/*.js --exit && npm run rollback",
    "devstart": "set NODE_ENV=development && npm run mockdata && babel-watch server/app.js",
    "start": "set NODE_ENV=production && npm run migrate && node server/bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "production": "npm run migrate && node server/bin/production",
    "migrate": "npm run build && npm run create-tables",
    "create-tables": "node dist/sqldatabase/models createAllTables",
    "insert": "node dist/sqldatabase/testdata insertData",
    "rollback": "node dist/sqldatabase/models dropAllTables",
    "mockdata": "npm run build && npm run rollback && npm run create-tables && npm run insert",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "generate-report": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debelistic/WayFarer.git"
  },
  "keywords": [
    "wayfarer",
    "bus",
    "transportation",
    "andela",
    "blp"
  ],
  "author": "Victor Awotidebe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/debelistic/WayFarer/issues"
  },
  "homepage": "https://github.com/debelistic/WayFarer#readme",
  "devDependencies": {
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-mocha": "^5.3.0"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "bcrypt": "^3.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "compression": "^1.7.4",
    "datauri": "^2.0.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-formidable": "^1.2.0",
    "helmet": "^3.16.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.0",
    "make-runnable": "^1.3.6",
    "mocha": "^6.1.3",
    "morgan": "^1.9.1",
    "nodemailer": "^6.1.0",
    "pg": "^7.9.0",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2"
  }
}
